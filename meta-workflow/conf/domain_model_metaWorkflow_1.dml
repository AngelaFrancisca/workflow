 valueType pt.utl.ist.fenix.tools.util.Strings as Strings {
    externalizeWith {
        String exportAsString();
    }

    internalizeWith importFromString();
}

 class WorkflowMetaType {
 	String name;
 	Integer processCounter;
 	Strings suporttedFileClasses;
 }
 
 class WorkflowMetaTypeDescription {
 	String description;
 	int version;
 	DateTime date;
 }
 
class WorkflowQueue {
	String name;
	String ojbConcreteClass;
} 

class WorkflowUserGroupQueue extends WorkflowQueue {

}

class WorkflowUnitQueue extends WorkflowQueue {
	
}

class Requestor {
	String ojbConcreteClass;
}

class ExternalRequestor extends Requestor {
	String name;
	String email;
}

relation WorkflowQueueWorkflowMetaType {
	WorkflowQueue playsRole queues {
		multiplicity *;
	}
	WorkflowMetaType playsRole metaType;
}

relation WorkflowMetaTypeDescription {
	WorkflowMetaType playsRole metaType;
	WorkflowMetaTypeDescription playsRole descriptions {
		multiplicity *;
	}
}


