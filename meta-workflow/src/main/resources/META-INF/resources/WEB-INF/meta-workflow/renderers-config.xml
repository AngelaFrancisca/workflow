<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE renderers-config SYSTEM "../fenix-renderers-config.dtd" []>
<renderers-config>
<renderer type="module.metaWorkflow.util.versioning.DiffUtil$Revision" layout="showDiff"
	class="module.metaWorkflow.presentationTier.renderers.RevisionRenderer">

	<property name="modifiedLineClass" value="highlightBox"/>
